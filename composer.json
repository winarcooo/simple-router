{
    "name": "winarcooo/simple-router",
    "description": "Simple router for personal used",
    "keywords": [
        "composer",
        "package",
        "router",
        "regex"
    ],
    "license": "MIT",
    "type": "library",
    "version": "1.0",
    "autoload": {
        "psr-4": {
            "Winarcooo\\SimpleRouter\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Andy Winarko",
            "email": "winarcooo@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1"
    },
    "require-dev": {
        "pestphp/pest": "^2.34"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
